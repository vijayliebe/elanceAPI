<div class="container-fluid">
    <div class="row">
        <div class="col-md-2 appLeftPanel">
            <ul class="nav ">
                <li class=""><a href="#"><b>Work Spaces by Org</b></a></li>
                <!--<li class=""><a href=#>Configuration</a></li>-->
            </ul>

            <div class="clearfix"></div>

            <ul class="nav ">
                <% for(var i = 0; i < orgs.length; i++) { %>
                <li class="org" data-org-ID="<%= orgs[i].org_id %>"><%= orgs[i].name %></li>

                    <ul class="nav ">
                        <% for(var j = 0; j < orgs[i].spaces.length; j++) { %>
                        <li class="workspace"><a href="/jobPostAutomation/<%= orgs[i].spaces[j].space_id %>" data-space-ID ="<%= orgs[i].spaces[j].space_id %>" data-org-ID="<%= orgs[i].spaces[j].org_id %>"><%= orgs[i].spaces[j].name %></a></li>
                        <% } %>
                    </ul>

                <% } %>
            </ul>

        </div>
        <div class="col-md-10 appRightPanel">
            <div class="col-md=12 appRightPanelHeader">Configuration</div>
            <div class="clearfix"></div>

            <ul class="nav ">
                <li class=""><a href="#">Left Panel contain list of Workspaces for you Podio account.</a></li>
                <li class=""><a href="#">Click to select any of workspace. Installation and further config. will happen in this workspace.</a></li>
            </ul>
        </div>
    </div>
</div>

<script>
    $('.workspace').click(function(){
        $(this).css("pointer-events","none");
    })
    </script>




